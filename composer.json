{
    "name": "mmoreram/doppo",
    "type": "library",
    "description": "Small Dependency Injection container",
    "keywords": ["php", "dependency injection"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": ">=5.3.7",
        "psr/log": "*"
    },
    "autoload": {
        "psr-4": {"Doppo\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Doppo\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
